@page "/search-news"
@inherits NewsBase

@if (!noArticlesFound)
{
    <h3>Found articles</h3>
}

<SearchBar OnSearch="SearchArticles" />
<News Articles="@Articles"
      OnLoadMoreArticles="LoadMoreArticles"
      IsLoadMoreButtonVisible="isLoadMoreButtonVisible" 
      NoArticlesFound="noArticlesFound"/>
